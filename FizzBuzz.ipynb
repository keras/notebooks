      "source": [
        "from itertools import islice\n",
        "def fizzbuzz():\n",
        "    def f():\n",
        "        f = 2,3,3,0,3,1,0,3\n",
        "        while True:\n",
        "            yield from f[1:] + f[::-1]\n",
        "    for ff, f in enumerate(f(), 1):\n",
        "        yield (['Fizz','Buzz','FizzBuzz']+[ff])[f]\n",
        "\n",
        "for n in islice(fizzbuzz(), 100):\n",
        "    print(n)"
      ],
